pole=[["*","*","*","*","*",],
     ["*","*","*","*","*",],
     ["*","*","*","*","*",],
     ["*","*","*","*","*",],
     ["*","*","*","*","*",]]
a=int(input("введите строку"))
s=int(input("введите столбец"))
pole[a][s] ="O"
for stroka in pole:
    for kletka in stroka:
        print(kletka,end="")
    print()
