n=""
while n!="КОНЕЦ":
    print("введите название страны")
    n=input()
    r="вас поздровляет"
    if n=="Россия"
          print(r, 'с новым годом из России: "с новым Годом!"')
    elif n=="италия"
          print(r, 'Бабо Натале из Италли"Feliceannonuovo!"')
    elif n=="нарвегия"
          print(r, 'Юлебукк из нарвегии "GottNyttar!"')
    elif n=="татарстан"
          print(r, 'кашбабай из татарстана "яна ел белэн"')
    elif n=="конец":
        break
    else:
        print("нет днных по этой стране")
print("cпосибо за понимание")
