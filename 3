num=input("введите логин :")
a=input("введите пароль :")
while num != "666"  or a != "1234":
    print("пароль или логин не верны")
    if num == "666" and a !="1234":
        print("пароль неверный")
    elif a=="1234" and num!="666":
        print("логин неверный")
    elif a!="1234" and num!="666":
         print("логин и пароль не верны")
    num=input("введите логин :")
    a=input("введите пароль :")
print("логин и пароль верный!")
